alias xup="xrdb -I$HOME ~/.Xresources"
alias chrome='chromium-browser'
alias gnvim='nvim-qt 2>&1 >/dev/null &'
alias e='nvim'

alias dls='xo ~/Downloads'
alias snaps="xo ~/snaps"

alias ..='cd ..'
alias ...='cd ../..'
alias ....='cd ../../..'
alias ~='cd ~'

alias xprop='xprop |less'
alias xo='__xo'

alias lsd="__sd"

alias gdbt='gdb -tui'
alias supervalgrind='valgrind -s --leak-check=full --show-reachable=yes'
